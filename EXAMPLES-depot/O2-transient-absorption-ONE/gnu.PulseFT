
if [[ "$2 " == " " ]]
then
	xlow="*"
	xhigh="*"
else
	xlow=$1
	xhigh=$2
fi



echo "set term x11
set style data lines

set logscale y

set xrange [$xlow:$xhigh]
set xlabel \"Energy (eV)\"
set ylabel \"FT \"

plot \"Dat/Pulseft.Dat\" using (27.2114*\$1):2 lw 4 notitle

set out \"pulseft.ps\"
set term post enhanced color solid 20
replot
" | gnuplot -persist &






