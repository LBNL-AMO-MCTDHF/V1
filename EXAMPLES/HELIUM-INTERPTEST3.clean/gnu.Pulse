
#set ylabel \"A(t) or V(t)\"

if [[ "$2 " == " " ]]
then
	low="*"
	high="*"
else
	low="$1"
	high="$2"
fi
	

echo "set term x11
set style data lines
set xrange [$low:$high]
set xlabel \"Time (au)\"
set ylabel \"A(t) \"
plot \"Dat/Pulse.Dat\" using 1:2 lw 4 notitle

set out \"pulse.ps\"
set term post enhanced color solid 20
replot
" | gnuplot -persist &




